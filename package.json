{
  "name": "react-measurements-ts",
  "private": false,
  "version": "1.2.0",
  "type": "module",
  "description": "A React component for measuring & annotating images. Rewritten with TypeScript.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YMNNs/react-measurements-ts.git"
  },
  "homepage": "https://ymnns.github.io/react-measurements-ts/",
  "main": "dist/react-measurements-ts.js",
  "author": {
    "email": "i@stager.top",
    "name": "dmsfrc"
  },
  "license": "MIT",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build:demo": "tsc && vite build",
    "build:lib": "tsc --p ./tsconfig.build.json && vite build --mode lib",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:lib-style": "stylelint \"lib/**/*.css\" --fix",
    "lint:src-style": "stylelint \"src/**/*.css\" --fix",
    "prettier:code": "prettier -c --ignore-unknown --write **/*",
    "prepare": "husky",
    "prepublishOnly": "npm run build:lib",
    "preview": "vite preview",
    "changelog-latest": "conventional-changelog -p angular -o CHANGELOG-latest.md && git add .",
    "version": "npm run changelog-latest"
  },
  "lint-staged": {
    "*.md": "lint-md -f",
    "**/*.{js,jsx,ts,tsx,cjs}": "eslint --fix",
    "**/*.css": "stylelint --fix",
    "**/*": "prettier -c --ignore-unknown --write"
  },
  "dependencies": {
    "draft-js": "^0.11.7"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@lint-md/cli": "^2.0.0",
    "@tsconfig/vite-react": "^3.0.2",
    "@types/draft-js": "^0.11.18",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "conventional-changelog": "^6.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-unicorn": "^53.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stylelint": "^16.6.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-lib-inject-css": "^2.1.1"
  }
}
